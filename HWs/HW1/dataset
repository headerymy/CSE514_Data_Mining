
class DataSet:

    '''
    dataset
    '''
    def __init__(self):
        self.rows = list()#data row
        self.title = list()#title

    '''
    add row
    '''
    def addrow(self,row):
        self.rows.append(row)

    '''
    set title
    '''
    def settitle(self,title):
        self.title = title

    '''
    get row number
    '''
    def getrowcount(self):
        return len(self.rows)

    '''
    get title
    '''
    def gettitle(self):
        return self.title

    '''
    get row data
    '''
    def getrows(self):
        return self.rows
